{
    "graphics": {
        "shaders": {
            "none": {
                "fragment_shader": "null",
                "vertex_shader": "null",
                "geometry_shader": "null"
            },
            "default": {
                "vertex_shader": "shaders\/vertex\/default.vertex",
                "geometry_shader": "null",
                "fragment_shader": "shaders\/fragment\/default.fragment"
            },
            "textured": {
                "vertex_shader": "shaders\/vertex\/textured.vertex",
                "geometry_shader": "null",
                "fragment_shader": "shaders\/fragment\/textured.fragment"
            }
        },
        "resolution": {
            "x": "860",
            "y": "640"
        },
        "textures": {
            "default": "assets\/default.bmp",
            "stone": "assets\/stone.bmp",
            "size": {
                "x": "50",
                "y": "50",
                "z": "4"
            },
            "list": [
                "default",
                "stone"
            ]
        }
    },
    "gameplay": {
        "tiles": {
            "default": {
                "texture": "default"
            },
            "list": [
                "default"
            ]
        }
    },
    "keys": {
        "Escape": {
            "up": "quit"
        },
        "Up": {
            "up": "camera.move.up = false",
            "down": "camera.move.up = true"
        },
        "Down": {
            "up": "camera.move.down = false",
            "down": "camera.move.down = true"
        },
        "Left": {
            "up": "camera.move.left = false",
            "down": "camera.move.left = true"
        },
        "Right": {
            "up": "camera.move.right = false",
            "down": "camera.move.right = true"
        },
        "Keypad +": {
            "up": "camera.move.in = false",
            "down": "camera.move.in = true"
        },
        "Keypad -": {
            "up": "camera.move.out = false",
            "down": "camera.move.out = true"
        },
        "Space": "unkown",
        "`": "toggleLines"
    },
    "mouse": {
        "wheel": {
            "scroll": {
                "up": "camera.step.in",
                "down": "camera.step.out"
            }
        }
    },
    "chunks": {
        "size": "256",
        "layers": "1"
    },
    "camera": {
        "speed": "1",
        "zspeed": "1",
        "z": "16"
    }
}
